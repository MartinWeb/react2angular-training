{
  "name": "react2angular",
  "private": true,
  "version": "0.0.0",
  "description": "A react2angular training application",
  "license": "MIT",
  "dependencies": {
    "angular": "1.8.x"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/angular/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server ./app -a localhost -p 8000 -c-1"
  }
}
